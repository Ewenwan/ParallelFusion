{
    "language"        : "PYTHON",
    "main-file"       : "optical_flow.py",
    "experiment-name" : "optical-flow",
    "likelihood"      : "gaussian",
    "resources": {
        "maximum-iterations" : 5,
        "max-run-time": "NA"
    },
    "space" : [
        {
            "name" : "exchange_amount",
            "type" : "continuous",
            "domain": "(0, 100)",
            "dimensionality": "1"
        },
        {   "name" : "num_proposals",
            "type" : "discrete",
            "domain": "(1, 2, 3, 4, 5)",
            "dimensionality": "1"
        },
        {
            "name" : "exchange_interval",
            "type" : "discrete",
            "domain": "(1, 2, 3, 4, 5)",
            "dimensionality": "1"
        }
    ],
    "constraints": [
      {
        "name": "cont_1",
        "constrain": "x[:, 0] - x[:, 1] + 1"
      }
    ],
    "output":{
        "verbosity": true
    }
}