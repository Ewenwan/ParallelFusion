{
    "language"        : "PYTHON",
    "main-file"       : "test.py",
    "experiment-name" : "simple-example",
    "likelihood"      : "gaussian",
    "resources": {
        "maximum-iterations" :  50,
        "max-run-time": "NA"
    },
    "space" : [
        {
            "name" : "x",
            "type" : "continuous",
            "domain": "(0, 5)",
             "dimensionality":1
