            "type" : "discrete",
            "domain": "(-5, 5)",
            "dimensionality":1
        }
    ],
    "constraints": [
      {
      "name": "cont_1",
      "constrain": "x[:, 1] - x[:, 0]"
      }
    ],
    "output":{
        "verbosity": true
    }
}